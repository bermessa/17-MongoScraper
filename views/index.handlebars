<div class="panel-body">
    <div class="row">
        <div class="col-md-12 scrapeResults">

            {{#each articles}}

            <p class="title">
                {{this.title}}

            </p>

            <p class="link">
                <a href="{{this.link}}" target="_blank" id="linkOne">Link to article</a> |
                <a class="clickSave" id="linkTwo" data-id={{this._id}}>Save Article</a>
            </p>

            <br> {{/each}}

        </div>
    </div>
</div>
